{
  "name": "database-kit",
  "main": "./dist-electron/main.js",
  "author": "zhuyongqi",
  "description": "tools for databases",
  "private": false,
  "version": "0.0.0",
  "type": "module",
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:7777/"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "start": "electron-forge start",
    "package": "rm -rf dist dist-electron && vite build && electron-forge package",
    "make:darwin": "rm -rf dist dist-electron && vite build && electron-forge make --platform=darwin --arch=arm64",
    "make:win": "rm -rf dist dist-electron && vite build && electron-forge make --platform=win32 --arch=x64",
    "publish": "vite build && electron-forge publish"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-collapsible": "^1.1.1",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-scroll-area": "^1.2.1",
    "@radix-ui/react-select": "^2.1.2",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.1",
    "@radix-ui/react-tooltip": "^1.1.4",
    "@radix-ui/react-visually-hidden": "^1.1.2",
    "@tabler/icons-react": "^3.31.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "electron-store": "^10.0.1",
    "framer-motion": "^12.0.0-alpha.1",
    "i18next": "^25.2.0",
    "i18next-browser-languagedetector": "^8.1.0",
    "lucide-react": "^0.468.0",
    "next-themes": "^0.4.6",
    "react": "^19.0.0",
    "react-compiler-runtime": "^19.1.0-rc.2",
    "react-diff-viewer": "latest",
    "react-dom": "^19.0.0",
    "react-i18next": "^15.5.2",
    "react-use": "^17.5.1",
    "sonner": "^2.0.3",
    "ssh2": "^1.16.0",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.2.0",
    "@electron-forge/maker-dmg": "^7.2.0",
    "@electron-forge/maker-squirrel": "^7.2.0",
    "@electron-forge/maker-zip": "^7.2.0",
    "@electron-forge/plugin-vite": "^7.8.1",
    "@electron-forge/publisher-github": "^7.2.0",
    "@electron-forge/shared-types": "^7.8.1",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/ssh2": "^1.15.1",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "babel-plugin-react-compiler": "^19.1.0-rc.2",
    "electron": "31.7.6",
    "eslint": "^9.16.0",
    "eslint-plugin-react-compiler": "^19.1.0-rc.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "postcss": "^8.4.49",
    "postcss-import": "^16.1.0",
    "tailwindcss": "^3.4.16",
    "typescript": "^5.7.2",
    "vite": "^5.4.11",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-electron-renderer": "^0.14.6",
    "vite-plugin-react": "^4.0.1"
  }
}
